{
    "functions": {
        "api/*.js": {
            "memory": 128,
            "maxDuration": 10
        }
    },
    "build": {
        "env": {
            "SUPABASE_URL": "@supabase_url",
            "SUPABASE_KEY": "@supabase_key"
        }
    },
    "env": {
        "NEXT_PUBLIC_SUPABASE_URL": "your_supabase_url",
        "NEXT_PUBLIC_SUPABASE_KEY": "your_supabase_key"
    }
}
